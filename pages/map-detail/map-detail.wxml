<view class="container">
  <text class="title">{{name}}</text>
  <text>{{description}}</text>
  <text>Specialty: {{specialty}}</text>
  <text class="action" bindtap='addresstap'>Address: {{address}}</text>
  <text class="action" bindtap='phonetap'>Phone number: {{tel_number}}</text>
  <text>Hours: {{hours}}</text>
  <text>Waiting time: {{waiting_time}}</text>
</view>
